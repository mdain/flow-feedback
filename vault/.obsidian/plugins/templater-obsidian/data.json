{
  "command_timeout": 5,
  "templates_folder": "04_Templates",
  "templates_pairs": [
    [
      "",
      ""
    ]
  ],
  "trigger_on_file_creation": true,
  "auto_jump_to_cursor": false,
  "enable_system_commands": false,
  "shell_path": "",
  "user_scripts_folder": "",
  "enable_folder_templates": true,
  "folder_templates": [
    {
      "folder": "02_Goals",
      "template": "04_Templates/Goal Template.md"
    },
    {
      "folder": "03_Tasks",
      "template": "04_Templates/Task Template.md"
    }
  ],
  "enable_file_templates": false,
  "file_templates": [
    {
      "regex": ".* ^Task - .*",
      "template": "04_Templates/Task Template.md"
    },
    {
      "regex": "^Goal - .*",
      "template": "04_Templates/Goal Template.md"
    }
  ],
  "syntax_highlighting": true,
  "syntax_highlighting_mobile": false,
  "enabled_templates_hotkeys": [
    ""
  ],
  "startup_templates": [
    ""
  ],
  "intellisense_render": 1
}